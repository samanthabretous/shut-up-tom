{
    "files": [
        "src/index.ts",
        "src/dashboard.ts",
        "src/greet.ts"
    ],
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "removeComments": false,
      "noImplicitAny": false,
      "suppressImplicitAnyIndexErrors": true
    },
    "exclude": [
      "node_modules"
    ]
}
